.st_share{display: flex;}
.st_share>button{flex:1;text-align: center;margin-top: 30rpx;background-color: transparent;font-size: 26rpx;color: #333333;}
.st_share image{width: 100rpx;height: 100rpx;display: block;margin:0 auto 10rpx;}
.btn_cancel{font-size: 30rpx;padding: 30rpx;border-top:1px solid #e5e5e5;}
