.st_record{margin:auto;font-size: 30rpx;position: relative;}
.st_record>image{height: 165rpx;width: 645rpx;display: block;margin:auto}
.st_record .title{color: #fff; position: absolute;font-size:55rpx;width: 100%;right: 0rpx;z-index: 2;top:50rpx;font-weight: 600;font-family:HYYaKuHeiW;}
.st_record .st_record_top{padding:30rpx 0;display: flex;}
.st_record .st_record_top text{flex: 1;}
.st_record .st_record_top text:nth-of-type(1){border-right:1px solid #7E8CA5;}
.st_table_body{background-color: #fff;border-radius:0 0 10rpx 10rpx;width: 645rpx;margin:auto;}
.st_record .st_table>view:nth-of-type(1){color:#FF3844;line-height:61rpx;background:rgba(255,247,247,1);border-bottom:none;}
.st_record .st_table view{border-bottom: 1px solid #eee;}
.st_record .st_table text{width: 33.3%;display: inline-block;line-height: 80rpx;}
.st_record .st_table_box >view:last-child {border-bottom: 0;}
.st_record .note{padding: 30rpx 20rpx;color: #B0B0B0;line-height: 40rpx;font-size: 24rpx;}

