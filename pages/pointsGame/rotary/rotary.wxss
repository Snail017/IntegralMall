.cu-custom, .cu-custom .cu-bar {
	background-color: #fff;
}
page {
	height: 100%;
}
#luckdraw_box {
	width: 690rpx;
	height: 690rpx;
	margin: 30rpx auto 0;
	position: relative;
	z-index: 22;
}

#luckdraw_back {
	width: 550rpx;
	height: 550rpx;
	text-align: center;
	position: relative;
	margin:40rpx auto 0;
}

#luckdraw_back .st-bg {
	display: block;
	width: 100%;
	height: 100%;
	object-position: center;
	object-fit: scale-down;
	transform-origin: center center;
}

.st_top {
	padding: 30rpx;
	overflow: hidden;
}

.st_parleft {
	position: relative;
	float: left;
}

.st_parleft image {
	width: 76rpx;
	height: 76rpx;
	display: inline-block;
	position: absolute;
	bottom: -4rpx;
	left: -20rpx;
}

.st_parleft text,
.st_parright {
	line-height: 65rpx;
	display: inline-block;
	padding: 0 30rpx 0 70rpx;
	color: #fff;
	background: rgba(255, 67, 63, 1);
	border-radius: 33px;
	font-size: 30rpx;
}

.st_parright {
	float: right;
	padding-left: 30rpx;
}

#luckdraw_pointer {
	width: 290rpx;
	height: 290rpx;
	position: absolute;
	left: 50%;
	margin-left: -145rpx;
	z-index: 999;
	top: 50%;
	margin-top: -210rpx;
	object-fit: scale-down;
}

#luckdraw_pointer image {
	display: block;
	width: 100%;
	height: 100%;
}

.st_body {
	min-height: calc(100% - 60px);
	background-image: url('https://ningshui-message.oss-cn-beijing.aliyuncs.com/1600408301001380676-rottry_bg.png');
	background-size: 100%;
	background-repeat: no-repeat;
	background-color: #ffcc4f;
	width: 100%;
	margin: 0;
}

.st_diz {
	width: 100%;
	height: 253rpx;
	margin-top: -190rpx;
	object-fit: scale-down;
}

.btn_start {
	width: 529rpx;
	height: 157rpx;
	margin: auto;
	display: block;
}

.st_text {
	color: rgba(146, 83, 27, 1);
	font-size: 26rpx;
	text-align: center;
	display: inline-block;
	margin: auto;
	width: 100%;
}

.st_rule {
	text-align: right;
	margin-right: 30rpx;
	padding-bottom: 40rpx;
}

.st_rule image {
	width: 32rpx;
	height: 32rpx;
	display: inline-table;
	vertical-align: text-bottom;
	margin-right: 5rpx;
}

.st_rule text {
	color: rgba(93, 53, 17, 1);
	font-size: 30rpx;
}

.rule_modal .text_con {
	overflow: auto;
	max-height: 551rpx;
}

.rule_modal .padding-xl {
	border-radius: 0 0 20rpx 20rpx;
}

.rule_modal rich-text {
	display: block;
	color: #000;
	text-align: left;
	line-height: 58rpx;
	font-size: 30rpx;
}

.rule_modal .hidemodal {
	margin: 53rpx auto 0;
}

.rule_modal .hidemodal image {
	width: 62rpx;
	height: 62rpx;
}

.rule_modal .cu-bar {
	flex-direction: column;
	justify-content: center;
}

.cu-bar .bar_img,
.bar_top .bar_img {
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	width: 140rpx;
	height: 140rpx;
	margin: -70rpx auto 25rpx;
	background: rgba(255, 255, 255, 1);
	border: 1px solid rgba(238, 238, 238, 1);
	border-radius: 25rpx;
}

