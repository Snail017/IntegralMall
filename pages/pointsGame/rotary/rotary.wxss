.st_bei image {
	width: 620rpx;
	height: 709rpx;
}

.st_bei_txt {
	position: absolute;
	top: 370rpx;
	left: 50%;
	width: 300rpx;
	margin-left: -150rpx;
}

.st_bei_txt view:nth-of-type(1) {
	color: rgba(245, 5, 36, 1);
