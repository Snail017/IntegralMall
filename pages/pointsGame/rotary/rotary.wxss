.st_caidan view:nth-last-of-type(1) {
	position: absolute;
	top: 250rpx;
	left: 260rpx;
	color: rgba(236, 38, 53, 1);
	font-weight: 600;
	width: 100rpx;
}

.st_caidan view:nth-last-of-type(1) text {
	display: block;
	font-size: 73rpx;
}

.st_caidan button {
	background-color: transparent;
	color: #fff;
